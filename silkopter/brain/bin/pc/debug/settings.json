{
    "hal": {
        "buses": {
            "I2C_Linux": {
                "name": "i2c0",
                "dev": "./i2c0"
            },
            "I2C_Linux": {
                "name": "i2c1",
                "dev": "./i2c1"
            }
        },
        "nodes": {
            "MPU9250": {
                "name": "imu",
                "bus": "i2c0",
                "imu_rate": 1000,
                "compass_rate": 100,
                "thermometer_rate": 10,
                "accelerometer_range": 4,
                "gyroscope_range": 500
            },
            "Magnetic_Field_Resampler": {
                "name": "imu-magnetic_field_resampler",
                "source_stream": "imu-compass/stream",
                "output_rate": 1000
            },
            "Acceleration_LPF": {
                "name": "imu-acceleration_lpf",
                "source_stream": "imu-accelerometer/stream",
                "poles": 2,
                "cutoff_frequency": 30
            },
            "Angular_Velocity_LPF": {
                "name": "imu-angular_velocity_lpf",
                "source_stream": "imu-gyroscope/stream",
                "poles": 2,
                "cutoff_frequency": 30
            },
            "Magnetic_Field_LPF": {
                "name": "imu-magnetic_field_lpf",
                "source_stream": "imu-magnetic_field_resampler/stream",
                "poles": 2,
                "cutoff_frequency": 30
            },
            "AHRS_Complimentary": {
                "name": "ahrs",
                "acceleration_stream": "imu-acceleration_lpf/stream",
                "angular_velocity_stream": "imu-angular_velocity_lpf/stream",
                "magnetic_field_stream": "imu-magnetic_field_lpf/stream"
            },
            "Inertial": {
                "name": "inertial",
                "reference_frame_stream": "ahrs/stream",
                "acceleration_stream": "imu-acceleration_lpf/stream"
            },
        }
    }
}